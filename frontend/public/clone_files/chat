!function(){window.addEventListener("message",function(t){t.data&&t.data["modal-content"]&&(document.getElementById("uchat-modal-dialog").innerHTML=t.data["modal-content"],document.getElementById("uchat-modal").style.display="")},!1);window.addEventListener("DOMContentLoaded",function(){var e,t;(e=document.createElement("div")).style.position="fixed",e.style.bottom="16px",e.style.right="16px",e.style.zIndex="100083",e.style.border=0,(t=document.createElement("img")).style.width="100px",t.style.height="100px",t.src="https://uidai.gov.in/templates/tjbase/static/media/Chatbot-Logo_New_jul_22.gif",document.body.appendChild(e),e.appendChild(t),e.onclick=function(){var t;e.style.display="none",document.getElementById("load_chat_script")?(document.getElementById("uchat").style.display="block",document.getElementById("uchat-iframe").contentWindow.postMessage({"uchat.open":!0},"https://aadhaarmitra.uidai.gov.in")):((t=document.createElement("script")).id="load_chat_script",t.src="https://aadhaarmitra.uidai.gov.in/load/chat?embed=web",document.body.appendChild(t))},window.addEventListener("message",function(t){0==t.origin.indexOf("https://aadhaarmitra.uidai.gov.in")&&t.data&&!t.data["uchat.open"]&&(document.getElementById("uchat").style.display="none",e.style.display="block")})})}();